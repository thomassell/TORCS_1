                android:layout_marginStart="175dp"
                android:layout_marginLeft="175dp"
                android:background="@drawable/rounded_corners"
                android:text="Flyer 10"
                android:textColor="@color/white"
                android:onClick="onClick" />

        </RelativeLayout>

    </ScrollView>
</FrameLayout>
