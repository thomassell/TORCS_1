                android:layout_width="wrap_content"
                android:layout_height="65dp"
                android:layout_marginStart="80dp"
                android:layout_marginLeft="80dp"
                android:layout_below="@+id/date_6"
                android:text="tba"
                android:textColor="@color/black" />
