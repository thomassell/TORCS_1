<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:fillViewport="true" >
    <RelativeLayout
        android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:paddingBottom="@dimen/activity_vertical_margin"
	    android:paddingLeft="@dimen/activity_horizontal_margin"
	    android:paddingRight="@dimen/activity_horizontal_margin"
	    android:paddingTop="@dimen/activity_vertical_margin"
	    android:background="@color/grey" >

    	<Button
    	    android:id="@+id/jr"
    	    style="?android:attr/buttonStyleSmall"
    	    android:layout_width="wrap_content"
    	    android:layout_height="wrap_content"
    	    android:layout_alignParentTop="true"
    	    android:layout_centerHorizontal="true"
    	    android:layout_marginTop="15dp"
    	    android:background="@drawable/rounded_corners_white"
    	    android:text="Jr. Beginner"
    	    android:textColor="@color/black"
    	    android:textStyle="bold"
    	    android:width="200dp"
    	    android:onClick="onClick" />
    	<Button
    	    android:id="@+id/fifty_cc"
    	    style="?android:attr/buttonStyleSmall"
    	    android:layout_width="wrap_content"
    	    android:layout_height="wrap_content"
    	    android:layout_centerHorizontal="true"
    	    android:layout_below="@+id/jr"
    	    android:layout_marginTop="15dp"
    	    android:background="@drawable/rounded_corners_white"
    	    android:text="50cc Open"
    	    android:textColor="@color/black"
    	    android:textStyle="bold"
    	    android:width="200dp"
    	    android:onClick="onClick" />
		<Button
			android:id="@+id/fifty_cc_mini"
			style="?android:attr/buttonStyleSmall"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_centerHorizontal="true"
			android:layout_below="@+id/fifty_cc"
			android:layout_marginTop="15dp"
			android:background="@drawable/rounded_corners_white"
			android:text="50cc Mini"
			android:textColor="@color/black"
			android:textStyle="bold"
			android:width="200dp"
			android:onClick="onClick" />
    	<Button
			android:id="@+id/sixty_cc_mini"
			style="?android:attr/buttonStyleSmall"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_centerHorizontal="true"
			android:layout_below="@+id/fifty_cc_mini"
			android:layout_marginTop="15dp"
			android:background="@drawable/rounded_corners_white"
			android:textStyle="bold"
			android:text="65cc Mini"
			android:textColor="@color/black"
			android:width="200dp"
			android:onClick="onClick" />
        <Button
    	    android:id="@+id/supermini"
    	    style="?android:attr/buttonStyleSmall"
    	    android:layout_width="wrap_content"
    	    android:layout_height="wrap_content"
    	    android:layout_centerHorizontal="true"
    	    android:layout_below="@+id/sixty_cc_mini"
    	    android:layout_marginTop="15dp"
    	    android:background="@drawable/rounded_corners_white"
    	    android:text="Supermini"
    	    android:textColor="@color/black"
    	    android:textStyle="bold"
    	    android:width="200dp"
    	    android:onClick="onClick" />
    	<Button
    	    android:id="@+id/wom_open"
    	    style="?android:attr/buttonStyleSmall"
    	    android:layout_width="wrap_content"
    	    android:layout_height="wrap_content"
    	    android:layout_centerHorizontal="true"
    	    android:layout_below="@+id/supermini"
    	    android:layout_marginTop="15dp"
    	    android:background="@drawable/rounded_corners_white"
    	    android:text="Women Open"
    	    android:textColor="@color/black"
    	    android:textStyle="bold"
    	    android:width="200dp"
    	    android:onClick="onClick" />
    	<Button
    	    android:id="@+id/double_decade"
    	    style="?android:attr/buttonStyleSmall"
    	    android:layout_width="wrap_content"
    	    android:layout_height="wrap_content"
    	    android:layout_centerHorizontal="true"
    	    android:layout_below="@+id/wom_open"
    	    android:layout_marginTop="15dp"
    	    android:background="@drawable/rounded_corners_white"
    	    android:text="Double Decade"
    	    android:textColor="@color/black"
    	    android:textStyle="bold"
    	    android:width="200dp"
    	    android:onClick="onClick" />
    	<Button
    	    android:id="@+id/sixty_plus"
    	    style="?android:attr/buttonStyleSmall"
    	    android:layout_width="wrap_content"
    	    android:layout_height="wrap_content"
    	    android:layout_centerHorizontal="true"
    	    android:layout_below="@+id/double_decade"
    	    android:layout_marginTop="15dp"
    	    android:background="@drawable/rounded_corners_white"
    	    android:text="Super Senior 60+"
    	    android:textColor="@color/black"
    	    android:textStyle="bold"
    	    android:width="200dp"
    	    android:onClick="onClick" />
		<Button
			android:id="@+id/sixty_five_plus"
			style="?android:attr/buttonStyleSmall"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_centerHorizontal="true"
			android:layout_below="@+id/sixty_plus"
			android:layout_marginTop="15dp"
			android:background="@drawable/rounded_corners_white"
			android:text="Super Senior 65+"
			android:textColor="@color/black"
			android:textStyle="bold"
			android:width="200dp"
			android:onClick="onClick" />
    	
	</RelativeLayout>
</ScrollView>
